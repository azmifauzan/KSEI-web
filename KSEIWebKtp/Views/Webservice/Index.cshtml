@model KSEIWebKtp.ViewModel.ServiceViewModel
@{ViewData["Title"] = "Web Service"; }
<h2>Cek Web Service</h2>

<div class="row" style="margin-top:20px;">
    <div class="col-md-2">&nbsp;</div>
    <div class="col-md-10">
        <p>&nbsp;</p>
        <form asp-action="Upload" enctype="multipart/form-data">
            <div class="form-horizontal">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="File_Upload" class="col-md-3 control-label"></label>
                    <div class="col-md-9">
                        <input name="files" type="file" />
                        <span asp-validation-for="File_Upload" class="text-danger"></span>
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-md-offset-3 col-md-9">
                        <input type="submit" value="Upload" class="btn btn-default" />
                    </div>
                </div>
            </div>
        </form>
    </div>

</div>